<root BTCPP_format="4" main_tree_to_execute="TakeOff">
    <BehaviorTree ID="TakeOff">
        <Sequence name="TakeOffSequence">
            <!-- 解锁飞机 -->
<!--            <LockControl state="1" />-->

            <!-- 设置飞行模式为OFFBOARD -->
            <FlightModeControl mode="9" />

            <!-- 执行起飞 -->
            <TakeOffAction alt="{alt}" />
        </Sequence>
    </BehaviorTree>
</root>