<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4" main_tree_to_execute="AutoTrace">
    <BehaviorTree ID="AutoTrace">
        <Sequence name="AutoTraceSequence">
            <!-- 设置目标点 -->
            <SetDestinationPoint dstParam="dstLoc" disParam="arvDis" />

            <!-- 进入OFFBOARD模式 -->
            <FlightModeControl mode="9" />

            <!-- 执行跟踪控制 -->
            <TraceAttackControl frame="1" command="3" current="1" />

            <!-- 检查到达目标 -->
            <CheckArriveDestination />
        </Sequence>
    </BehaviorTree>
</root>